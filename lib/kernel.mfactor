!-*- mode: factor -*-
USING: ;
IN: kernel

: shift ( x n -- y ) dup 0 > [ asl ] [ neg asr ] if ;

! basic arithmetic and logic operations

: >= ( n1 n2 -- ? ) 2dup > [ 2drop t ] [ = ] if ;

: t ( -- t ) 1 ;
: f ( -- f ) 0 ;

: and ( obj1 obj2 -- ? ) over ? ;
: or ( obj1 obj2 -- - ) dupd ? ;
: not ( ? -- \? ) 0 1 ? ;

